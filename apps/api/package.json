{
  "name": "@mono/api",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "api:start": "prisma generate && nodemon -w src/* -x 'ts-node src/api.ts'",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed --preview-feature",
    "db:start": "docker run --rm -i --name=api-db -p 5432:5432 -v api_data:/var/lib/postgresql/data -e POSTGRES_USER='test' -e POSTGRES_PASSWORD='test' -e POSTGRES_DB='api' postgres",
    "start": "concurrently -kn=api,db 'pnpm:api:start' 'pnpm:db:start'"
  },
  "dependencies": {
    "@mono/analytics": "workspace:*",
    "@mono/types": "workspace:*",
    "@prisma/client": "2.27.0-integration-fix-sdk-pnpm.2",
    "cors": "^2.8.5",
    "execa": "^5.0.0",
    "express": "^4.17.1",
    "prisma": "2.27.0-integration-fix-sdk-pnpm.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "private": true
}
